{
	"$schema": "http://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"title": "Header",
			"name": "header",
			"area": "header"
		},
		{
			"title": "Footer",
			"name": "footer",
			"area": "footer"
		},
		{
			"title": "Main Sidebar",
			"name": "sidebar",
			"area": "uncategorized"
		},
		{
			"title": "Top Sidebar",
			"name": "sidebar-top",
			"area": "uncategorized"
		},
		{
			"title": "Bottom Sidebar",
			"name": "sidebar-bottom",
			"area": "uncategorized"
		}
	],
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#EB6D2F",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FFD832",
					"name": "Secondary"
				},
				{
					"slug": "success",
					"color": "#7dc855",
					"name": "Success"
				},
				{
					"slug": "warning",
					"color": "#ffc107",
					"name": "Warning"
				},
				{
					"slug": "danger",
					"color": "#dc3545",
					"name": "Danger"
				},
				{
					"slug": "info",
					"color": "#0dcaf0",
					"name": "Info"
				},
				{
					"slug": "light",
					"color": "#F8F7F1",
					"name": "Light"
				},
				{
					"slug": "dark",
					"color": "#5A3733",
					"name": "Dark"
				}
			],
			"gradients": [],
			"link": true,
			"custom": true,
			"customGradient": true,
			"corePalette": false,
			"coreGradients": false
		},
		"layout": {
			"contentSize": "1320px",
			"wideSize": null
		}
	},
	"styles": {
		"blocks": {
			"core/paragraph": {
				"typography": {
					"lineHeight": "1.7"
				}
			},
			"core/button": {
				"typography": {
					"fontFamily": "var:preset|font-family|button",
					"fontSize": "var:preset|font-size|normal",
					"fontWeight": "700"
				},
				"border": {
					"width": "1px",
					"radius": "50px"
				},
				"spacing": {
					"padding": {
						"top": ".8rem",
						"bottom": ".8rem",
						"left": "2.5rem",
						"right": "2.5rem"
					}
				}
			}
		},
		"typography": {
			"fontSize": "var:preset|font-size|normal",
			"fontFamily": "var:preset|font-family|base"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "Signika",
					"fontWeight": "700"
				}
			},
			"h2": {
				"color": {
					"text": "#121212"
				},
				"typography": {
					"fontFamily": "Signika",
					"fontWeight": "700"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "Signika",
					"fontWeight": "700"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "Signika",
					"fontWeight": "700"
				}
			}
		}
	}
}