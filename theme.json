{
	"$schema": "http://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"title": "Header",
			"name": "header",
			"area": "header"
		},
		{
			"title": "Header (Dark)",
			"name": "header-dark",
			"area": "header"
		},
		{
			"title": "Header (Logo left)",
			"name": "header-left",
			"area": "header"
		},
		{
			"title": "Header (with Search)",
			"name": "header-search",
			"area": "header"
		},
		{
			"title": "Footer",
			"name": "footer",
			"area": "footer"
		},
		{
			"title": "Footer (Dark)",
			"name": "footer-dark",
			"area": "footer"
		},
		{
			"title": "Main Sidebar",
			"name": "sidebar",
			"area": "uncategorized"
		},
		{
			"title": "Banner: Shop",
			"name": "banner",
			"area": "uncategorized"
		},
		{
			"title": "Header: Top",
			"name": "header-top",
			"area": "header"
		},
		{
			"title": "Footer: Top",
			"name": "footer-top",
			"area": "footer"
		}
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"name": "Base",
					"slug": "base",
					"fontFamily": "var(--wp--font-system)"
				},
				{
					"name": "Signika",
					"slug": "signika",
					"fontFamily": "Signika"
				}
			]
		},
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#7dc855",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FFD832",
					"name": "Secondary"
				},
				{
					"slug": "success",
					"color": "#7dc855",
					"name": "Success"
				},
				{
					"slug": "warning",
					"color": "#FFD832",
					"name": "Warning"
				},
				{
					"slug": "danger",
					"color": "#C11501",
					"name": "Danger"
				},
				{
					"slug": "info",
					"color": "#0dcaf0",
					"name": "Info"
				},
				{
					"slug": "light",
					"color": "#F8F7F1",
					"name": "Light"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "dark",
					"color": "#5A3733",
					"name": "Dark"
				},
				{
					"slug": "accent",
					"color": "#f1f7ff",
					"name": "Accent"
				}
			],
			"gradients": [],
			"link": true,
			"custom": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false
		},
		"layout": {
			"contentSize": "1320px",
			"wideSize": null
		}
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|signika",
					"fontWeight": "700"
				},
				"spacing": {
					"margin": {
						"bottom": "1rem"
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"lineHeight": "1.7"
				}
			},
			"core/button": {
				"typography": {
					"fontFamily": "var:preset|font-family|button",
					"fontSize": "var:preset|font-size|normal",
					"fontWeight": "700"
				},
				"border": {
					"width": "1px",
					"radius": "50px"
				},
				"spacing": {
					"padding": {
						"top": ".8rem",
						"bottom": ".8rem",
						"left": "2.5rem",
						"right": "2.5rem"
					}
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			}
		},
		"typography": {
			"fontSize": "var:preset|font-size|normal",
			"fontFamily": "var:preset|font-family|base"
		}
	}
}